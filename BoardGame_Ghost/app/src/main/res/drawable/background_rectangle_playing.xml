<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    android:id = "@+id/rectangle_background_playing">

    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape
            android:shape="rectangle">
            <stroke
                android:width="5dp"
                android:color="#4472C4" />

            <solid android:color="#DAE3F3" />
        </shape>
    </item>

</layer-list>